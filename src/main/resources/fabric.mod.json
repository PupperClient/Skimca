{
  "schemaVersion": 1,
  "id": "skimca",
  "version": "${version}",
  "name": "skimca",
  "description": "A Minecraft mod with bindings for Skija",
  "authors": ["oneachina"],
  "contact": {},
  "license": "GPL-3.0",
  "icon": "assets/skimca/logo.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "cn.pupperclient.skimca.SkimcaClient"
    ],
    "main": [
      "cn.pupperclient.skimca.SkimcaClient"
    ]
  },
  "mixins": [
    "skimca.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": "*",
    "minecraft": "${minecraft_version}"
  },

  "custom": {
    "modmenu": {
      "badges": [ "library", "deprecated" ],
      "links": {
        "modmenu.discord": "https://discord.gg/jEGF5fb",
        "modmenu.source": "https://github.com/PupperClient/Skimca",
        "modmenu.website": "https://pupperclient.github.io/Pupper-Website/",
        "modmenu.issues": "https://github.com/PupperClient/Skimca/issues"
      },
      "authors": [
        "oneachina"
      ],
      "environment": "client",
      "update_checker": false
    }
  }
}
